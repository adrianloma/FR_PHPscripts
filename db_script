create table lesson_tek(
	lesson_id VARCHAR(10) NOT NULL,
	tek1 VARCHAR(6),
	tek2 VARCHAR(6),
	tek3 VARCHAR(6),
	tek4 VARCHAR(6),
	tek5 VARCHAR(6),
	tek6 VARCHAR(6),
	tek7 VARCHAR(6),
	tek8 VARCHAR(6),
	tek9 VARCHAR(6),
	tek10 VARCHAR(6),
	PRIMARY KEY(lesson_id)
);

#LE FALTA DEFINIR TAMANOS DE VARCHAR
create table contents(
	question_id INT NOT NULL,
	unit INT NOT NULL,
	chapter INT NOT NULL,
	lesson INT NOT NULL,
	c1 VARCHAR,
	c2 VARCHAR,
	c3 VARCHAR,
	c4 VARCHAR,
	c5 VARCHAR,
	c6 VARCHAR,
	c7 VARCHAR,
	c8 VARCHAR,
	c9 VARCHAR,
	c10 VARCHAR,
	c11 VARCHAR,
	c12 VARCHAR,
	PRIMARY KEY(question_id)

);
	

create table question_tek(
	question_id INT NOT NULL,
	tek1 VARCHAR,
	tek2 VARCHAR,
	tek3 VARCHAR,
	tek4 VARCHAR,
	tek5 VARCHAR,
	tek6 VARCHAR,
	tek7 VARCHAR,
	tek8 VARCHAR,
	tek9 VARCHAR,
	tek10 VARCHAR,
	PRIMARY KEY(question_id),
	FOREIGN KEY(question_id) REFERENCES contents(question_id)
	ON DELETE CASCADE
);


