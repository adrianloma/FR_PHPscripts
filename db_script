create database FreedomRun;
use FreedomRun;

create table district(
	email VARCHAR(100) NOT NULL,
	name VARCHAR(60) NOT NULL,
	password VARCHAR(60) NOT NULL,
	district_id INT NOT NULL AUTO_INCREMENT,
	licenses INT,
	PRIMARY KEY ( email ),
	KEY (district_id)
	
);

create table teachers(
	email VARCHAR(100) NOT NULL,
	teacher_id INT NOT NULL AUTO_INCREMENT,
	fname VARCHAR(50) NOT NULL,
	lname VARCHAR(50) NOT NULL,
	password VARCHAR(60) NOT NULL,
	district_email VARCHAR(60) NOT NULL,
	PRIMARY KEY ( email ),
	FOREIGN KEY (district_email) REFERENCES district(email),
	KEY (teacher_id)
	
);

create table validations(
	teacher_email VARCHAR(100) NOT NULL,
	validation VARCHAR(10) NOT NULL,
	group_id INT NOT NULL AUTO_INCREMENT,
	max_students INT,
	PRIMARY KEY (validation),
	FOREIGN KEY (teacher_email) REFERENCES teachers(email)
	ON UPDATE CASCADE,
	KEY (group_id)
);

create table students(
	email VARCHAR(60) NOT NULL,
	student_id INT NOT NULL AUTO_INCREMENT,
	validation VARCHAR(6) NOT NULL,
	fname VARCHAR(50) NOT NULL,
	lname VARCHAR(50) NOT NULL,
	gender CHAR(1) NOT NULL,	
	language TINYINT(1) NOT NULL DEFAULT 1,
	sound TINYINT(1) NOT NULL DEFAULT 1,
	avatar INT NOT NULL DEFAULT 1,
	password VARCHAR(60) NOT NULL,
	PRIMARY KEY(email),
	FOREIGN KEY(validation) REFERENCES validations(validation),
	KEY (student_id)
);

